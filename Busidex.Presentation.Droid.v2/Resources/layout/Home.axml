<!--<ImageButton
        p1:src="@drawable/Exit"
        p1:layout_width="45dp"
        p1:layout_height="45dp"
        p1:id="@+id/btnLogout"
        p1:scaleType="fitXY"
        p1:background="@android:color/transparent"
        p1:layout_alignParentBottom="true"
        p1:layout_alignParentLeft="true"
        p1:layout_marginLeft="5dp" />-->
<!--NOTIFICATIONS-->
    <ImageView
        p1:src="@drawable/mybusidexicon"
        p1:layout_width="50dp"
        p1:layout_height="50dp"
        p1:id="@+id/btnSharedCardsNotification"
        p1:scaleType="fitXY"
        p1:layout_alignParentBottom="true"
        p1:textAlignment="gravity"
        p1:layout_marginBottom="3dp"
        p1:layout_marginRight="15dp"
        p1:layout_alignParentRight="true" />
    <TextView
        p1:text="1"
        p1:textAppearance="?android:attr/textAppearanceSmall"
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:layout_toLeftOf="@id/btnSharedCardsNotification"
        p1:id="@+id/txtNotificationCount"
        p1:textColor="@color/buttonFontColor"
        p1:padding="3dp"
        p1:layout_alignParentBottom="true"
        p1:layout_marginRight="-46dp"
        p1:layout_marginBottom="28dp"
        p1:textStyle="bold"
        p1:textSize="12pt" />
</RelativeLayout>